{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "accountid": {
      "type": "integer"
    },
    "productid": {
      "type": "integer"
    },
    "day": {
      "type": "string",
      "format": "date-time"
    },
    "price": {
      "type": "integer"
    },
    "coupon": {
      "type": "string"
    }
  },
  "required": ["accountid", "productid", "day", "price", "coupon"],
  "additionalProperties": false
}
